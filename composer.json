{
    "name": "bazofather/bale-php",
    "description": "bale messenger bot sdk in php",
    "homepage": "https://github.com/bazofather/bale-php",
    "keywords": [
        "bot",
        "bale",
        "messenger",
        "bazofather",
        "botfather",
        "telegram bot"
    ],
    "support": {
        "issues": "https://github.com/bazofather/bale-php/issues",
        "source": "https://github.com/bazofather/bale-php"
    },
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "bazofather/interface": "^0.1.2"
    },
    "autoload": {
        "psr-4": {
            "Bazofather\\Bale\\": "./src"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT"
}
